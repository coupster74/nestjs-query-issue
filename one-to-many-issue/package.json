{
  "name": "nestjsqueryissue",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nodemon --config nodemon.json",
    "start:dev:db": "docker compose up",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm_nosource": "yarn run build && npx typeorm",
    "typeorm": "yarn run build && npx typeorm -d dist/db/data-source.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate",
    "typeorm:migration:create": "npm run typeorm_nosource -- migration:create",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeoem:migration:revert": "npm run typeorm -- migration:revert",
    "postinstall": "tsc --sourceMap false"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/server": "4.5.0",
    "@nestjs/apollo": "11.0.4",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "9.3.12",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.3.12",
    "@nestjs/graphql": "11.0.4",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "9.3.12",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ptc-org/nestjs-query-core": "^2.3.0",
    "@ptc-org/nestjs-query-graphql": "^2.3.0",
    "@ptc-org/nestjs-query-sequelize": "^2.3.0",
    "@ptc-org/nestjs-query-typeorm": "^2.3.0",
    "@types/multer": "^1.4.7",
    "@types/react": "^18.0.28",
    "archiver": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-fetch": "^3.1.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.12.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.41",
    "nestjs-typeorm-paginate": "^4.0.3",
    "node-mailjet": "^6.0.2",
    "pg": "^8.10.0",
    "react": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5",
    "subscriptions-transport-ws": "^0.11.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.12",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.9",
    "@types/express": "4.17.17",
    "@types/jest": "^27.0.1",
    "@types/node": "18.15.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "nodemon": "^2.0.14",
    "prettier": "2.8.4",
    "source-map-support": "^0.5.20",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
